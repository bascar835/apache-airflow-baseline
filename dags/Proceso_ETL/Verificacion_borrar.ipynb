    "import sqlite3\n",
    "import pandas as pd\n",
    "\n",
    "# Conectar a la base de datos SQLite\n",
    "conn = sqlite3.connect('output/database.sqlite')\n",
    "query = \"SELECT * FROM etl_results\"\n",
    "\n",
    "# Leer la tabla en un DataFrame de pandas\n",
    "df = pd.read_sql(query, conn)\n",
    "\n",
    "# Mostrar las primeras filas\n",
    "df.head()\n"
